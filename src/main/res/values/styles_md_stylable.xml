    <item name="mdSecondaryTextColor_warnPalette_dark">?attr/mdSecondaryTextColor_accentPalette_dark</item>
    <item name="mdSecondaryTextColor_warnPalette_light">?attr/mdSecondaryTextColor_accentPalette_light</item>
    <item name="mdSecondaryTextColor_backgroundPalette">#8A000000</item>
    <item name="mdSecondaryTextColor_backgroundPalette_dark">?attr/mdSecondaryTextColor_backgroundPalette</item>
    <item name="mdSecondaryTextColor_backgroundPalette_light">?attr/mdSecondaryTextColor_backgroundPalette</item>

    <!-- textColor for all palettes -->
    <item name="mdHintTextColor_primaryPalette">#42000000</item>
    <item name="mdHintTextColor_primaryPalette_dark">#42FFFFFF</item>
    <item name="mdHintTextColor_primaryPalette_light">?attr/mdHintTextColor_primaryPalette</item>
