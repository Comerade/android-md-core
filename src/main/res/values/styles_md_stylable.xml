    <item name="mdHintTextColor_warnPalette_dark">?attr/mdHintTextColor_accentPalette_dark</item>
    <item name="mdHintTextColor_warnPalette_light">?attr/mdHintTextColor_accentPalette_light</item>
    <item name="mdHintTextColor_backgroundPalette">#42000000</item>
    <item name="mdHintTextColor_backgroundPalette_dark">?attr/mdHintTextColor_backgroundPalette</item>
    <item name="mdHintTextColor_backgroundPalette_light">?attr/mdHintTextColor_backgroundPalette</item>

    <!-- dividerColor for all palettes -->
    <item name="mdDividerColor_primaryPalette">#1E000000</item>
    <item name="mdDividerColor_primaryPalette_dark">#1EFFFFFF</item>
    <item name="mdDividerColor_primaryPalette_light">?attr/mdDividerColor_primaryPalette</item>
    <item name="mdDividerColor_accentPalette">#1EFFFFFF</item>
    <item name="mdDividerColor_accentPalette_dark">?attr/mdDividerColor_accentPalette</item>
    <item name="mdDividerColor_accentPalette_light">#1E000000</item>
    <item name="mdDividerColor_warnPalette">?attr/mdDividerColor_accentPalette</item>
    <item name="mdDividerColor_warnPalette_dark">?attr/mdDividerColor_accentPalette_dark</item>
    <item name="mdDividerColor_warnPalette_light">?attr/mdDividerColor_accentPalette_light</item>
    <item name="mdDividerColor_backgroundPalette">#1E000000</item>
    <item name="mdDividerColor_backgroundPalette_dark">?attr/mdDividerColor_backgroundPalette</item>
    <item name="mdDividerColor_backgroundPalette_light">?attr/mdDividerColor_backgroundPalette</item>

    <item name="mdFontSize_button">14sp</item>
    <item name="mdFontSize_caption">12sp</item>
    <item name="mdFontSize_body1">14sp</item>
    <item name="mdFontSize_body2">14sp</item>
    <item name="mdFontSize_subhead">16sp</item>
    <item name="mdFontSize_title">20sp</item>
    <item name="mdFontSize_headline">24sp</item>
    <item name="mdFontSize_display1">34sp</item>
