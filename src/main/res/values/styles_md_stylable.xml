    <item name="mdLineHeight_body1">20sp</item>
    <item name="mdLineHeight_body2">24sp</item>
    <item name="mdLineHeight_subhead">?attr/mdLineHeight_subhead1</item>
    <item name="mdLineHeight_subhead1">24sp</item>
    <item name="mdLineHeight_subhead2">28sp</item>
    <item name="mdLineHeight_headline">32sp</item>
    <item name="mdLineHeight_display1">40sp</item>
    <item name="mdLineHeight_display2">48sp</item>

    <item name="mdLayout_spacing_lg">24dp</item>
    <item name="mdLayout_spacing">16dp</item>
    <item name="mdLayout_spacing_sm">8dp</item>
    <item name="mdLayout_spacing_xs">4dp</item>
    <item name="mdLayout_spacing_xxs">2dp</item>
    <item name="mdLayout_spaceBetweenContentAreas">8dp</item>
    <item name="mdLayout_contentAssociatedWithIconOrAvatarLeftMargin">72dp</item>

    <item name="mdDrawerLayout_translucentNavigationBackground">@null</item>
    <item name="mdDrawerLayout_fitsSystemWindows">false</item>

    <!-- override material style -->
    <item name="colorPrimary">?attr/mdColor_primaryPalette</item>
    <item name="colorPrimaryDark">?attr/mdColor_primaryPalette_dark</item>
    <item name="colorAccent">?attr/mdColor_accentPalette</item>

  </style>

