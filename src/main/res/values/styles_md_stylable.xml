<resources>

  <style name="MdStylable" parent="Theme.AppCompat.Light.NoActionBar">

    <!-- used to represent primary / main interface elements for a user -->
    <item name="mdColor_primaryPalette">#03A9F4</item>
    <item name="mdColor_primaryPalette_dark">#0288D1</item>
    <item name="mdColor_primaryPalette_light">#B3E5FC</item>

    <!-- used to represent secondary interface elements for a user -->
    <item name="mdColor_accentPalette">#E91E63</item>
    <item name="mdColor_accentPalette_dark">?attr/mdColor_accentPalette</item>
    <item name="mdColor_accentPalette_light">?attr/mdColor_accentPalette</item>

    <!-- used to represent interface elements that the user should be careful of -->
    <item name="mdColor_warnPalette">?attr/mdColor_warnPalette</item>
    <item name="mdColor_warnPalette_dark">?attr/mdColor_warnPalette</item>
    <item name="mdColor_warnPalette_light">?attr/mdColor_warnPalette</item>

    <!-- used to represent application background for a user -->
    <item name="mdColor_backgroundPalette">#F5F5F5</item>
    <item name="mdColor_backgroundPalette_dark">?attr/mdColor_backgroundPalette</item>
    <item name="mdColor_backgroundPalette_light">?attr/mdColor_backgroundPalette</item>

    <!-- pressed state color for all palettes -->
    <item name="mdColor_primaryPalette_pressed">?attr/mdColor_primaryPalette_dark</item>
    <item name="mdColor_primaryPalette_dark_pressed">?attr/mdColor_primaryPalette_light</item>
    <item name="mdColor_primaryPalette_light_pressed">?attr/mdColor_primaryPalette</item>
    <item name="mdColor_accentPalette_pressed">?attr/mdColor_accentPalette_dark</item>
    <item name="mdColor_accentPalette_dark_pressed">?attr/mdColor_accentPalette_light</item>
    <item name="mdColor_accentPalette_light_pressed">?attr/mdColor_accentPalette</item>
    <item name="mdColor_warnPalette_pressed">?attr/mdColor_warnPalette_dark</item>
    <item name="mdColor_warnPalette_dark_pressed">?attr/mdColor_warnPalette_light</item>
    <item name="mdColor_warnPalette_light_pressed">?attr/mdColor_warnPalette</item>
    <item name="mdColor_backgroundPalette_pressed">?attr/mdColor_backgroundPalette_dark</item>
    <item name="mdColor_backgroundPalette_dark_pressed">?attr/mdColor_backgroundPalette_light</item>
    <item name="mdColor_backgroundPalette_light_pressed">?attr/mdColor_backgroundPalette</item>

    <!-- Dark text on light backgrounds:  87%:DE 54%:8A 26%:42 12%:1E -->
    <!-- White text on dark backgrounds: 100%:FF 70%:B3 30%:4C 12%:1E -->

    <!-- textColor for all palettes -->
    <item name="mdTextColor_primaryPalette">#DE000000</item>
    <item name="mdTextColor_primaryPalette_dark">#DEFFFFFF</item>
    <item name="mdTextColor_primaryPalette_light">?attr/mdTextColor_primaryPalette</item>
    <item name="mdTextColor_accentPalette">#DEFFFFFF</item>
    <item name="mdTextColor_accentPalette_dark">?attr/mdTextColor_accentPalette</item>
    <item name="mdTextColor_accentPalette_light">#DE000000</item>
    <item name="mdTextColor_warnPalette">?attr/mdTextColor_accentPalette</item>
    <item name="mdTextColor_warnPalette_dark">?attr/mdTextColor_accentPalette_dark</item>
    <item name="mdTextColor_warnPalette_light">?attr/mdTextColor_accentPalette_light</item>
    <item name="mdTextColor_backgroundPalette">#DE000000</item>
    <item name="mdTextColor_backgroundPalette_dark">?attr/mdTextColor_backgroundPalette</item>
    <item name="mdTextColor_backgroundPalette_light">?attr/mdTextColor_backgroundPalette</item>

    <!-- secondary textColor for all palettes -->
    <item name="mdSecondaryTextColor_primaryPalette">#8A000000</item>
    <item name="mdSecondaryTextColor_primaryPalette_dark">#8AFFFFFF</item>
    <item name="mdSecondaryTextColor_primaryPalette_light">?attr/mdSecondaryTextColor_primaryPalette</item>
    <item name="mdSecondaryTextColor_accentPalette">#8AFFFFFF</item>
    <item name="mdSecondaryTextColor_accentPalette_dark">?attr/mdSecondaryTextColor_accentPalette</item>
